<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
                           http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context-3.0.xsd">

    <context:annotation-config/>
    <context:component-scan base-package="org.carlspring.strongbox"/>

    <bean name="usersDao" class="org.carlspring.strongbox.dao.xml.impl.UsersDaoImpl">
        <property name=""
    </bean>

    <bean name="userResolver" class="org.carlspring.strongbox.security.jaas.xml.XMLUserRealm">
        <property name="usersDao" ref="usersDao"/>
    </bean>

    <bean name="userManager" class="org.carlspring.strongbox.security.jaas.managers.UserManager" />

    <bean name="userParser" class="org.carlspring.strongbox.xml.parsers.UserParser" />

    <bean name="authorizationManager" class="org.carlspring.strongbox.security.jaas.managers.AuthorizationManager"
          init-method="load"/>
    
</beans>
